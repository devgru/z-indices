{
  "name": "z-indices",
  "version": "0.0.4",
  "description": "A simple javascript utility for calculating z-index",
  "scripts": {
    "build": "webpack -p",
    "test": "jest --watchAll",
    "prepublish": "npm run build"
  },
  "files": [
    "dist.js"
  ],
  "main": "dist.js",
  "repository": "devgru/z-indices",
  "author": "Dmitriy Semyushkin <git@devg.ru> (https://devg.ru)",
  "license": "MIT",
  "keywords": [
    "z-index",
    "z-indices"
  ],
  "devDependencies": {
    "babel-jest": "18",
    "babel-loader": "6",
    "babel-preset-es2015": "6",
    "jest": "18",
    "webpack": "2"
  },
  "jest": {
    "collectCoverageFrom": [
      "lib/**/*.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
